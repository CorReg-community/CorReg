year <- sub("-.*", "", meta$Date) 
note <- sprintf("R package version %s", meta$Version) 
bibentry(bibtype = "Manual", title = "{CorReg}: Linear Regression Based on Linear Structure Between Covariates", author = c(person("Clement", "Thery")), year = year, note = note, url = "https://CRAN.R-project.org/package=CorReg")
